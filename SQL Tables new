
46.

CREATE TABLE clients (
    id INT PRIMARY KEY,
    mac_address VARCHAR(255)
);

CREATE TABLE streams (
    client_id INT,
    title VARCHAR(255),
    quality ENUM('240p','360p','480p','720p','1080p','1440p','2160p'),
    traffic INT,
    FOREIGN KEY (client_id) REFERENCES clients(id)
);


