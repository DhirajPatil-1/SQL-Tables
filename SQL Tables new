-- Cities table
CREATE TABLE cities (
    id INT PRIMARY KEY,
    name VARCHAR(255)
);

-- Banners table
CREATE TABLE banners (
    city_id INT,
    width SMALLINT,
    height SMALLINT,
    FOREIGN KEY (city_id) REFERENCES cities(id)
);

