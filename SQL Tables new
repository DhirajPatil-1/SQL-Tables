CREATE TABLE accounts (
    id INT PRIMARY KEY,
    first_name VARCHAR(255),
    last_name VARCHAR(255),
    iban VARCHAR(255)
);

CREATE TABLE declarations (
    account_id INT,
    quarter ENUM('Q1', 'Q2', 'Q3', 'Q4'),
    income DECIMAL(7, 2),
    FOREIGN KEY (account_id) REFERENCES accounts(id)
);

